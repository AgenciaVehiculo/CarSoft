	<queryString>
		<![CDATA[SELECT
     persona.`Id_persona` AS persona_Id_persona,
     persona.`Nombre` AS persona_Nombre,
     persona.`Apellido` AS persona_Apellido,
     persona.`telefono` AS persona_telefono,
     persona.`direccion` AS persona_direccion,
     persona.`correo_electroncio` AS persona_correo_electroncio,
     persona.`ID_tipo_documento` AS persona_ID_tipo_documento,
     persona.`documento_id` AS persona_documento_id,
     cliente.`Id_Persona` AS cliente_Id_Persona,
     cliente.`Id_cliente` AS cliente_Id_cliente,
     cliente.`estado` AS cliente_estado,
     cliente.`fecha_ingreso_sistema` AS cliente_fecha_ingreso_sistema,
     tipo_documento.`ID_tipo_documento` AS tipo_documento_ID_tipo_documento,
     tipo_documento.`Documento` AS tipo_documento_Documento,
     tipo_documento.`estado` AS tipo_documento_estado
FROM
     `persona` persona INNER JOIN `cliente` cliente ON persona.`Id_persona` = cliente.`Id_Persona`
     INNER JOIN `tipo_documento` tipo_documento ON persona.`ID_tipo_documento` = tipo_documento.`ID_tipo_documento`]]>
	</queryString>
